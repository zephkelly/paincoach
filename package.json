{
    "name": "monorepo",
    "type": "module",
    "version": "0.0.1",
    "main": "./nuxt.config.ts",
    "scripts": {
        "dev": "pnpm --filter main dev",

        "dev:p": "pnpm --filter patient dev",
        "install:p": "pnpm --filter patient install",

        "dev:lib": "pnpm --filter library dev",
        "install:lib": "pnpm --filter library install",

        "i": "pnpm --recursive install"
    }
  }